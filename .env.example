# Cloud API Keys for DataExtract RAG
# Get your FREE API keys from:
# - Groq: https://console.groq.com
# - Cohere: https://dashboard.cohere.com
# - Cloudinary: https://cloudinary.com

# Groq API (for LLM and Whisper transcription)
GROQ_API_KEY=your_groq_api_key_here

# Cohere API (for embeddings)
COHERE_API_KEY=your_cohere_api_key_here

# Cloudinary (for file storage - optional)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Toggle cloud vs local processing (set to false to use local)
USE_CLOUD_LLM=true
USE_CLOUD_WHISPER=true
USE_CLOUD_EMBEDDINGS=true
USE_CLOUDINARY=false
